@model TasimaTalebiDto

<div class="text-center">
    <div class="display-6">Talep Oluþtur</div>
</div>

<form method="post" asp-action="Index" enctype="multipart/form-data">

    <div class="text-danger" asp-validation-summary="All"></div>

    <div class="form-group my-3">
        <label asp-for="Aciklama">Açýklama</label>
        <input asp-for="Aciklama" class="form-control" />
    </div>
    <div class="form-group my-3">
        <label asp-for="Baslangic">Baþlangýç Noktasý</label>
        <input asp-for="Baslangic" class="form-control" />
    </div>
    <div class="form-group my-3">
        <label asp-for="Bitis">Bitiþ Noktasý</label>
        <input asp-for="Bitis" class="form-control" />
    </div>
    <div class="form-group my-3">
        <label asp-for="Mesafe">Aradaki Mesafe</label>
        <input asp-for="Mesafe" class="form-control" />
    </div>
    <div class="form-group my-3">
        <label asp-for="EsyaImgUrl">Eþyalarýn Resmi</label>
        <input name="file" type="file" class="form-control" />
    </div>
    <div class="form-group my-3">
        <div class="btn-group">
            <button class="btn btn-primary" type="submit">
                <i class="fa fa-save"></i>
                Save
            </button>
        </div>
    </div>
</form>